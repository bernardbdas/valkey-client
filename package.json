{
  "name": "valkey-client",
  "version": "1.0.0",
  "description": "A simple Express server",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "concurrently \"rimraf dist\" \"npx tsc\"",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc -w\" \"nodemon dist/server.js\"",
    "dev": "npx tsc -w & nodemon dist/server.js"
  },
  "author": "Simplify3x",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-parser": "^1.4.7",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^5.1.1",
    "jose": "^5.2.4",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^4.17.2",
    "morgan": "^1.10.0",
    "rimraf": "^5.0.5",
    "valkey": "^0.0.1",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^18.19.31",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^28.1.3",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
